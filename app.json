{
    "expo": {
        "name": "Medicals",
        "slug": "rn-medicals",
        "privacy": "public",
        "platforms": ["ios", "android"],
        "version": "1.0.1",
        "orientation": "default",
        "icon": "./assets/rn-icon.png",
        "splash": {
            "image": "./assets/rn-splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#242832"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.rususadev.medicals_rn_app",
            "buildNumber": "1.0.1"
        },
        "android": {
            "package": "com.rususadev.medicals_rn_app",
            "versionCode": 1,
            "permissions": ["WRITE_EXTERNAL_STORAGE"]
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
